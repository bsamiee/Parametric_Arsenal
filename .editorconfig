root = true
dotnet_code_quality.api_surface = public, internal, private                                     # Analyze the entire API surface, not just public

# --- Global defaults ------------------------------------------------
[*]
end_of_line                                                     = lf
insert_final_newline                                            = true
charset                                                         = utf-8
trim_trailing_whitespace                                        = true

# ====================================================================
# C# style, quality, analyzers
# ====================================================================
[*.cs]

# --- Analyzer floors ------------------------------------------------
dotnet_analyzer_diagnostic.category-Style.severity              = error
dotnet_analyzer_diagnostic.category-Design.severity             = error
dotnet_analyzer_diagnostic.category-Performance.severity        = error
dotnet_analyzer_diagnostic.category-Reliability.severity        = error
dotnet_analyzer_diagnostic.category-Maintainability.severity    = error
dotnet_analyzer_diagnostic.category-Security.severity           = error
dotnet_analyzer_diagnostic.category-Usage.severity              = error
dotnet_analyzer_diagnostic.category-Naming.severity             = error
dotnet_analyzer_diagnostic.category-Globalization.severity      = error
dotnet_analyzer_diagnostic.category-Interoperability.severity   = error
dotnet_analyzer_diagnostic.category-Portability.severity        = error
dotnet_analyzer_diagnostic.category-Documentation.severity      = error

# --- Namespaces -----------------------------------------------------
csharp_style_namespace_declarations                             = file_scoped:error             # Enforce file-scoped; block-scoped will be flagged
dotnet_style_namespace_match_folder                             = true:error                    # Consistency between folder and namespace

# --- Accessibility and explicitness ---------------------------------
dotnet_style_require_accessibility_modifiers                    = always:error
dotnet_diagnostic.IDE0040.severity                              = error                         # add accessibility modifier
# Qualification for clarity
dotnet_style_qualification_for_field                            = true:error
dotnet_style_qualification_for_property                         = true:error
dotnet_style_qualification_for_method                           = true:error
dotnet_style_qualification_for_event                            = true:error

# --- Using directives -----------------------------------------------
csharp_using_directive_placement                                = outside_namespace:error
dotnet_sort_system_directives_first                             = true:error
dotnet_separate_import_directive_groups                         = true:error

# --- Strong typing over var -----------------------------------------
csharp_style_var_for_built_in_types                             = false:error
csharp_style_var_when_type_is_apparent                          = false:error
csharp_style_var_elsewhere                                      = false:error
dotnet_diagnostic.IDE0008.severity                              = error                         # use explicit type

# --- Language keywords for types ------------------------------------
dotnet_style_predefined_type_for_locals_parameters_members      = true:error
dotnet_style_predefined_type_for_member_access                  = true:error

# --- Readonly, purity, immutability ---------------------------------
dotnet_style_readonly_field                                     = true:error                    # IDE0044
csharp_style_prefer_readonly_struct                             = true:error
csharp_style_prefer_readonly_record_struct                      = true:error
dotnet_diagnostic.CA1822.severity                               = error                         # mark members static when possible
dotnet_diagnostic.CA1852.severity                               = error                         # seal types when appropriate

# --- Using declarations and resource handling -----------------------
csharp_prefer_simple_using_statement                            = true:error                    # IDE0063
dotnet_diagnostic.IDE0063.severity                              = error
dotnet_diagnostic.CA2000.severity                               = error                         # dispose objects before losing scope
dotnet_diagnostic.CA2213.severity                               = error                         # disposable fields should be disposed

# --- Modern C# feature set (density, clarity) -----------------------
# Expression/statement forms that remove boilerplate
csharp_style_throw_expression                                   = true:error
dotnet_style_prefer_auto_properties                             = true:error
dotnet_style_prefer_inferred_tuple_names                        = true:error
dotnet_style_prefer_inferred_anonymous_type_member_names        = true:error
dotnet_style_prefer_is_null_check_over_reference_equality_method = true:error
# Prefer static where capture is unnecessary (perf + clarity)
csharp_prefer_static_local_function                             = true:error                    # IDE0062
csharp_prefer_static_anonymous_function                         = true:error                    # IDE0320
# Records / tuple swap (concise semantics when applicable)
csharp_style_prefer_record                                      = true:error
csharp_style_prefer_tuple_swap                                  = true:error
# Initializers / collection expressions
dotnet_style_prefer_object_initializer                          = true:error
dotnet_style_prefer_collection_initializer                      = true:error
dotnet_style_prefer_collection_expression                       = when_types_loosely_match:error
dotnet_diagnostic.IDE0300.severity                              = error
dotnet_diagnostic.IDE0301.severity                              = error
dotnet_diagnostic.IDE0302.severity                              = error
dotnet_diagnostic.IDE0303.severity                              = error
dotnet_diagnostic.IDE0304.severity                              = error
dotnet_diagnostic.IDE0305.severity                              = error
dotnet_diagnostic.IDE0090.severity                              = error                         # Target-typed new
# Pattern matching / switch / not/relational patterns
csharp_style_pattern_matching_over_is_with_cast_check           = true:error
csharp_style_prefer_switch_expression                           = true:error
csharp_style_prefer_relational_pattern                          = true:error
csharp_style_prefer_not_pattern                                 = true:error
# Tuples / indices / ranges
csharp_style_deconstructed_variable_declaration                 = true:error
csharp_style_prefer_index_operator                              = true:error
csharp_style_prefer_range_operator                              = true:error
# Expression-bodied members (single-line)
csharp_style_expression_bodied_methods                          = when_on_single_line:error
csharp_style_expression_bodied_properties                       = when_on_single_line:error
csharp_style_expression_bodied_operators                        = when_on_single_line:error
csharp_style_expression_bodied_indexers                         = when_on_single_line:error
csharp_style_expression_bodied_accessors                        = when_on_single_line:error
csharp_style_expression_bodied_constructors                     = when_on_single_line:error
# Simplifications
dotnet_style_prefer_conditional_expression_over_assignment      = true:error
dotnet_style_prefer_conditional_expression_over_return          = true:error
dotnet_style_prefer_coalesce_expression                         = true:error
dotnet_style_null_propagation                                   = true:error
dotnet_style_prefer_simplified_boolean_expressions              = true:error
dotnet_style_prefer_simplified_interpolation                    = true:error
csharp_style_prefer_simple_default_expression                   = true:error
dotnet_style_prefer_compound_assignment                         = true:error
# nameof, default literal, UTF-8 literals
dotnet_style_prefer_nameof_expression                           = true:error
csharp_style_prefer_default_literal                             = true:error
csharp_style_prefer_utf8_string_literals                        = true:error
# Named arguments for clarity
csharp_style_prefer_named_arguments                             = true:error
# Method group / local vs anonymous
csharp_style_prefer_method_group_conversion                     = true:error
csharp_style_prefer_local_over_anonymous_function               = true:error

# --- SOLID enforcement ----------------------------------------------
dotnet_diagnostic.CA1000.severity                               = warning                       # Do not declare static members on generic types
dotnet_diagnostic.CA1008.severity                               = warning                       # Enums should have zero value
dotnet_diagnostic.CA1027.severity                               = warning                       # Mark enums with FlagsAttribute
dotnet_diagnostic.CA1024.severity                               = warning                       # Use properties where appropriate
dotnet_diagnostic.CA1033.severity                               = warning                       # Interface methods should be callable by child types

# --- Braces, operators, whitespace discipline -----------------------
csharp_prefer_braces                                            = when_multiline:suggestion
dotnet_style_operator_placement_when_wrapping                   = end_of_line:error
csharp_style_allow_embedded_statements_on_same_line_experimental = false:error
dotnet_style_allow_multiple_blank_lines_experimental            = false:error
dotnet_style_allow_statement_immediately_after_block_experimental = true:error                  # Allow (and enforce) no blank line between block and subsequent statement
csharp_new_line_before_open_brace                               = none                          # K&R: open brace on same line for all constructs
dotnet_diagnostic.IDE0011.severity                              = none                          # allow omitted braces
dotnet_diagnostic.IDE2001.severity                              = none                          # allow inline variable declarations
# Keep compact one-liners where legal
csharp_preserve_single_line_statements                          = true
csharp_preserve_single_line_blocks                              = true
csharp_new_line_between_query_expression_clauses                = false                         # Avoid extra line breaks in query expressions
# Tighten spaces (concise but readable)
csharp_space_after_keywords_in_control_flow_statements          = true
csharp_space_between_parentheses                                = false
csharp_space_after_cast                                         = false
csharp_space_around_binary_operators                            = before_and_after
# Avoid extra new lines before else/catch/finally
csharp_new_line_before_else                                     = false
csharp_new_line_before_catch                                    = false
csharp_new_line_before_finally                                  = false

# --- Parentheses preferences ----------------------------------------
dotnet_style_parentheses_in_arithmetic_binary_operators         = never_if_unnecessary:error
dotnet_style_parentheses_in_relational_binary_operators         = never_if_unnecessary:error
dotnet_style_parentheses_in_other_binary_operators              = never_if_unnecessary:error
dotnet_style_parentheses_in_other_operators                     = never_if_unnecessary:error
dotnet_diagnostic.IDE0047.severity                              = none

# --- Primary constructors preference --------------------------------
csharp_style_prefer_primary_constructors                        = true:error
dotnet_diagnostic.IDE0290.severity                              = error

# --- Nullability enforcement ----------------------------------------
dotnet_diagnostic.NX0001.severity                               = none
dotnet_diagnostic.NX0002.severity                               = none
dotnet_diagnostic.NX0003.severity                               = none
dotnet_diagnostic.NX0004.severity                               = none
dotnet_diagnostic.CS8600.severity                               = error                         # Converting null literal to non-nullable
dotnet_diagnostic.CS8602.severity                               = error                         # Dereference of a possibly null ref
dotnet_diagnostic.CS8603.severity                               = error                         # Possible null return
dotnet_diagnostic.CS8618.severity                               = error                         # Uninitialized non-nullable field
dotnet_diagnostic.CS8620.severity                               = error                         # Nullability mismatch in parameter
dotnet_diagnostic.CS8625.severity                               = error                         # Cannot convert null literal to non-nullable
dotnet_diagnostic.CS8629.severity                               = error                         # Nullable value type may be null
dotnet_diagnostic.CS8766.severity                               = error                         # Nullability mismatch in return type
dotnet_diagnostic.CS8767.severity                               = error                         # Nullability mismatch in explicit interface impl
dotnet_diagnostic.CS8774.severity                               = error                         # Member must have a non-null value

# --- HCA rules for safety & perf ------------------------------------
dotnet_diagnostic.CA1062.severity                               = error                         # validate public method args for null
dotnet_diagnostic.CA1305.severity                               = error                         # specify IFormatProvider
dotnet_diagnostic.CA1309.severity                               = error                         # use ordinal/stringcomparison
dotnet_diagnostic.CA1508.severity                               = error                         # avoid dead conditional code
dotnet_diagnostic.CA1827.severity                               = error                         # use Any() over Count()>0
dotnet_diagnostic.IDE0058.severity                              = error                         # value is never used
dotnet_diagnostic.IDE0059.severity                              = error                         # unnecessary assignment
dotnet_diagnostic.IDE0062.severity                              = error                         # local function can be static
dotnet_diagnostic.IDE0070.severity                              = error                         # use HashCode.Combine
dotnet_diagnostic.IDE0074.severity                              = error                         # use compound assignment when possible
dotnet_diagnostic.IDE0005.severity                              = none                          # remove unnecessary using (requires GenerateDocumentationFile)
# Do NOT require ConfigureAwait in a UI-hosted plugin; manage context explicitly where needed
dotnet_diagnostic.CA2007.severity                               = none                          # "Do not directly await a Task" — intentionally disabled for plugins
# Platform & exception safety
dotnet_diagnostic.CA1416.severity                               = error                         # Validate platform-specific API usage
dotnet_diagnostic.CA1031.severity                               = error                         # Do not catch general Exception
dotnet_diagnostic.CA2214.severity                               = error                         # Do not call overridable methods in constructors
# Async contracts & tokens
dotnet_diagnostic.CA2012.severity                               = error                         # Use ValueTask correctly
dotnet_diagnostic.CA2016.severity                               = error                         # Forward CancellationToken to async methods
# Don’t ignore method results
dotnet_diagnostic.CA1806.severity                               = error

# --- Third‑party analyzers ------------------------------------------

# Meziantou.Analyzer: enforce named arguments, string comparison
dotnet_diagnostic.MA0003.severity                               = warning                       # add parameter name to improve readability
dotnet_diagnostic.MA0004.severity                               = error                         # use StringComparison (e.g., Ordinal/Invariant)
# Roslynator - picked for safety and density; rest follow category floors.
dotnet_diagnostic.RCS1001.severity                              = none                          # add braces suggestion disabled
dotnet_diagnostic.RCS1213.severity                              = error                         # unused parameter
dotnet_diagnostic.RCS1197.severity                              = warning                       # optimize StringBuilder usage
# IDisposableAnalyzers (ownership correctness)
dotnet_diagnostic.IDISP001.severity                             = error                         # Dispose created
dotnet_diagnostic.IDISP002.severity                             = error                         # Dispose member
dotnet_diagnostic.IDISP006.severity                             = error                         # Implement IDisposable
# Microsoft.VisualStudio.Threading.Analyzers (async correctness in a UI host)
dotnet_diagnostic.VSTHRD100.severity                            = error                         # Avoid async void
dotnet_diagnostic.VSTHRD103.severity                            = error                         # Call async methods with await
dotnet_diagnostic.VSTHRD200.severity                            = error                         # Use JoinableTaskFactory appropriately
# AsyncFixer (reduce “function spam” and misuse of async)
dotnet_diagnostic.AsyncFixer01.severity                         = error                         # Unnecessary async/await
dotnet_diagnostic.AsyncFixer02.severity                         = error                         # Blocking ops in async method
dotnet_diagnostic.AsyncFixer03.severity                         = error                         # async void fire-and-forget
dotnet_diagnostic.AsyncFixer04.severity                         = error                         # Fire-and-forget in using
dotnet_diagnostic.AsyncFixer05.severity                         = error                         # Nested Task => unwrapped

# --- Rhino/Grasshopper plugin biases --------------------------------
csharp_style_prefer_top_level_statements                        = false:error                  # Favor explicitness and predictability in plugins

# --- Naming: rules and styles ---------------------------------------
# Symbols
dotnet_naming_symbols.interfaces.applicable_kinds               = interface
dotnet_naming_symbols.interfaces.applicable_accessibilities     = *
dotnet_naming_symbols.type_parameters.applicable_kinds          = type_parameter
dotnet_naming_symbols.type_parameters.applicable_accessibilities = *
dotnet_naming_symbols.private_fields.applicable_kinds           = field
dotnet_naming_symbols.private_fields.applicable_accessibilities = private
dotnet_naming_symbols.constants.applicable_kinds                = field
dotnet_naming_symbols.constants.applicable_accessibilities      = *
dotnet_naming_symbols.constants.required_modifiers              = const
dotnet_naming_symbols.async_methods.applicable_kinds            = method
dotnet_naming_symbols.async_methods.applicable_accessibilities  = *
dotnet_naming_symbols.async_methods.required_modifiers          = async
# Styles
dotnet_naming_style.pascal_case.capitalization                  = pascal_case
dotnet_naming_style.camel_case.capitalization                   = camel_case
dotnet_naming_style.prefix_I.capitalization                     = pascal_case
dotnet_naming_style.prefix_I.required_prefix                    = I
dotnet_naming_style.prefix_T.capitalization                     = pascal_case
dotnet_naming_style.prefix_T.required_prefix                    = T
dotnet_naming_style.underscore_camel.capitalization             = camel_case
dotnet_naming_style.underscore_camel.required_prefix            = _
dotnet_naming_style.ends_with_Async.capitalization              = pascal_case
dotnet_naming_style.ends_with_Async.required_suffix             = Async
# Rules
dotnet_naming_rule.interfaces_must_start_with_I.symbols         = interfaces
dotnet_naming_rule.interfaces_must_start_with_I.style           = prefix_I
dotnet_naming_rule.interfaces_must_start_with_I.severity        = error

dotnet_naming_rule.type_parameters_start_with_T.symbols         = type_parameters
dotnet_naming_rule.type_parameters_start_with_T.style           = prefix_T
dotnet_naming_rule.type_parameters_start_with_T.severity        = error

dotnet_naming_rule.private_fields_underscore_camel.symbols      = private_fields
dotnet_naming_rule.private_fields_underscore_camel.style        = underscore_camel
dotnet_naming_rule.private_fields_underscore_camel.severity     = error

dotnet_naming_rule.constants_pascal_case.symbols                = constants
dotnet_naming_rule.constants_pascal_case.style                  = pascal_case
dotnet_naming_rule.constants_pascal_case.severity               = error

dotnet_naming_rule.async_methods_end_with_Async.symbols         = async_methods
dotnet_naming_rule.async_methods_end_with_Async.style           = ends_with_Async
dotnet_naming_rule.async_methods_end_with_Async.severity        = error

